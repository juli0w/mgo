<% if @company.profile.institutional.present? and @company.premium? %>
  <%= render "layouts/full/tab/inicial" %>
<% end %>

<% if @company.premium? %>
  <% @company.albums.each do |album| %>
    <%= render "layouts/full/tab/fotos", album: album %>
  <% end %>
<% end %>

<%= render "layouts/full/tab/contato" %>
